<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="Ellipse" x:Key="CBlack">
        <Setter Property="Fill" Value="Black"/>
        <Setter Property="Width" Value="60" />
        <Setter Property="Height" Value="60" />
        <Setter Property="Margin" Value="0,2.5,0,0" />
        <Setter Property="Canvas.Top" Value="12"/>
        <Setter Property="Canvas.Left" Value="200"/>
    </Style>

    <Style x:Key="CWhite" TargetType="Ellipse">
        <Setter Property="Fill" Value="White"/>
        <Setter Property="Width" Value="60" />
        <Setter Property="Height" Value="60" />
        <Setter Property="Margin" Value="0,2.5,0,0" />
        <Setter Property="Canvas.Top" Value="12"/>
        <Setter Property="Canvas.Right" Value="200"/>
    </Style>

</ResourceDictionary>